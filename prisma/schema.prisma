generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Track {
  guildId      String
  queueIndex   Int
  service      String
  title        String
  url          String
  user         String
  thumbnailUrl String
  lengthInSec  Int
  id           String @id @default(cuid())

  @@unique([guildId, queueIndex], name: "queuePosition")
}
