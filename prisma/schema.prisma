// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Track {
  guildId      String
  queueIndex   Int
  service      String
  title        String
  url          String
  user         String
  thumbnailUrl String
  lengthInSec  Int

  @@unique(fields: [guildId, queueIndex], name: "queue_id")
}
