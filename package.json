{
  "name": "beatnik",
  "version": "4.2.0",
  "description": "modern and simple Discord music bot with easy-to-use commands",
  "author": "vphoebe",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "npm run typecheck && tsx build.mjs",
    "commands": "tsx src/deploy-commands.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "dev": "tsx watch src/beatnik.ts",
    "docker:build": "docker build --platform linux/amd64 -t nickseman/beatnik:dev .",
    "docker:up": "DOCKER_DEFAULT_PLATFORM=linux/amd64 docker compose -f 'docker-compose.dev.yml' up",
    "format": "prettier --write .",
    "next:build": "docker build --platform linux/amd64 -f Dockerfile.production -t nickseman/beatnik:next .",
    "next:push": "docker push nickseman/beatnik:next",
    "next:up": "DOCKER_DEFAULT_PLATFORM=linux/amd64 docker compose -f 'docker-compose.next.yml' up",
    "preinstall": "npx only-allow npm",
    "release:build": "docker build --platform linux/amd64 -f Dockerfile.production -t nickseman/beatnik:latest .",
    "release:push": "docker push nickseman/beatnik:latest",
    "start": "npm run db:deploy && node ./build/deploy-commands.mjs && node ./build/beatnik.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@prisma/client": "^6.8.2",
    "bgutils-js": "^3.2.0",
    "discord.js": "^14.20.0",
    "dotenv": "^16.4.5",
    "jsdom": "^26.1.0",
    "node-libcurl-ja3": "^5.0.3",
    "prisma": "^6.8.2",
    "sodium-native": "^5.0.6",
    "sqlite3": "^5.1.7",
    "youtubei.js": "^15.0.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.27.0",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.15.6",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  }
}
