{
  "name": "beatnik",
  "version": "3.1.1",
  "description": "modern and simple Discord music bot with easy-to-use commands",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "dev:deployglobal": "ts-node src/deploy-global-commands.ts",
    "build": "tsc --build --clean && tsc --build",
    "build:docker": "npm run build && docker build -t beatnik-dev .",
    "build:dockerx64": "npm run build && docker buildx build --platform linux/amd64 -t beatnik-dev-x64 .",
    "start": "pm2-runtime ecosystem.config.js"
  },
  "author": "vphoebe",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.4.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.7.0",
    "@discordjs/voice": "^0.15.0",
    "@keyv/sqlite": "^3.5.2",
    "discord-api-types": "^0.33.5",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.1",
    "keyv": "^4.3.0",
    "sodium-native": "^3.3.0",
    "sqlite3": "^5.0.8",
    "youtube-search-without-api-key": "^1.0.7",
    "ytdl-core": "^4.11.4",
    "ytpl": "^2.3.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.4"
  }
}
